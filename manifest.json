{
  "manifest_version": 2,
 
  "name": "Gmail Sidebar",
  "version": "0.1.0",
  "description": "Keep Gmail left bar hidden",
 
  "content_scripts": [{
	"css": ["styles.css"],
  "js": ["content.js"],
  "matches": ["https://mail.google.com/mail/*"],
	"run_at": "document_idle",
	"all_frames": true
  }]
 
}